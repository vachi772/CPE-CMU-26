#include <iostream>
using namespace std;
int main()
{
	cout << "-----------0o--------------0o-------------\n";
	cout << "--------o0----0o---------0o--0o-----------\n";
	cout << "----------0o----0o------0o----0o----------\n";
	cout <<"------------o0---o0-o0-0o---0o------------\n"; 
	cout << "----------o0------------------0o----------\n"; 
	cout << "-------o0--------------------------0o------\n"; 
	cout << "----o0------------------------------0o----\n"; 
	cout << "--o0----------------------------------0o--\n"; 
	cout << "o0--------------------------------------0o\n"; 
	cout << "o0--------------------------------------0o\n"; 
	cout << "o0-----------a-------------a------------0o\n"; 
	cout << "o0--------------------------------------0o\n"; 
	cout << "-o0---------.-------U---------.--------0o-\n"; 
	cout << "--o0---------------------------------0o---\n"; 
	cout << "----o000------------------------000o------\n";
	cout << "--------0o--0o--0o-0o--0o--0o"; 
	return 0;
}
